<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#8C0000" /> <!-- Fill color (red) -->
            <corners android:radius="50dp" /> <!-- Corner radius (half of the width and height to make it round) -->
            <size android:width="80dp" android:height="80dp" /> <!-- Size of the shape -->
        </shape>
    </item>
    <item android:drawable="@drawable/re" />
</layer-list>
